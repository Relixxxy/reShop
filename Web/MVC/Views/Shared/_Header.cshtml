
@model IEnumerable<MVC.ViewModels.Header>

<div class="esh-header">
    <div class="container">
        @foreach (var header in @Model)
        {
            <a class="esh-header-back" asp-area=string.Empty asp-controller="@header.Controller" asp-action="Index">@header.Text</a>
        }
    </div>
</div>
